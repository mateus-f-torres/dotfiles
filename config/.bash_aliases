# sync dotfiles and configurations
alias dotsync="$HOME/projects/dotfiles/dotsync.sh"

# https://github.com/sharkdp/fd
alias fd=fdfind

# one word => full development environment setup automation
alias dialog="$HOME/dialog/dialog-scripts/tmux.sh"

# quickfix for WSL's random DNS resolution error
alias dnsfix="$HOME/projects/dotfiles/scripts/dnsfix.sh"

# easy toggle solarized theme
alias sol="$HOME/projects/dotfiles/sol.sh"
